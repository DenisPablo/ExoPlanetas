# Configuración del Proyecto ExoPlanetas
# Copia este archivo como .env y ajusta los valores según tu entorno

# Rutas de datos
DATASET_PATH=datasets/kepler.csv
OUTPUT_DIR=data/
MODELS_DIR=models/

# Parámetros del modelo
LEARNING_RATE=0.05
MAX_LEAF_NODES=31
MIN_SAMPLES_LEAF=20
EARLY_STOPPING=true

# Configuración de la API
APP_NAME=Exoplanet Classifier API
APP_VERSION=1.0.0
DEBUG=false

# Configuración de logging
LOG_LEVEL=INFO
